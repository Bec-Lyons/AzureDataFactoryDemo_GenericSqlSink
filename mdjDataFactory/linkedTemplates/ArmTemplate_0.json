						"folderPath": {
							"value": "@dataset().azureBlobSingleCSVFolderPath",
							"type": "Expression"
						},
						"container": {
							"value": "@dataset().azureBlobSingleCSVContainerName",
							"type": "Expression"
						}
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
