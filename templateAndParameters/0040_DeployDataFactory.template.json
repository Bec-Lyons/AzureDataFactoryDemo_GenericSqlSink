                            "expression": {
                                "value": "@pipeline().parameters.truncatePriorToCopy",
                                "type": "Expression"
                            },
                            "ifTrueActivities": [
                                {
                                    "name": "Truncate Sink Table Prior to Copy",
                                    "type": "SqlServerStoredProcedure",
                                    "dependsOn": [],
